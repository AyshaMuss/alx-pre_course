/* creating new repository in command line */
echo "# alx-pre_course-" >> README.md
git init
git add README.md
git commit -m "My first commit"
git branch -M main
git remote add origin http://github.com/Alx-pre_course.git
git push -u origin main

/*pushing an existing repository from command line*/
git remote add origin http://github.com/AyshaMuss/pre_course.git
git branch -M main
git pull origin main
git push -u origin main
